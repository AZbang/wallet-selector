{
  "root": "packages/nightly-connect",
  "sourceRoot": "packages/nightly-connect/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "targetDependencies": {
        "build": [{
          "target": "build",
          "projects": "dependencies"
        }]
      },
      "options": {
        "outputPath": "dist/packages/nightly-connect",
        "main": "packages/nightly-connect/src/index.ts",
        "tsConfig": "packages/nightly-connect/tsconfig.lib.json",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          "packages/nightly-connect/*.md",
          "packages/nightly-connect/assets/*"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/nightly-connect/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nightly-connect"],
      "options": {
        "jestConfig": "packages/nightly-connect/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": ["bridge-wallet"]
}
