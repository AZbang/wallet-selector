{
  "root": "packages/ledger-wallet",
  "sourceRoot": "packages/ledger-wallet/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ledger-wallet",
        "main": "packages/ledger-wallet/src/index.ts",
        "tsConfig": "packages/ledger-wallet/tsconfig.lib.json",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": ["packages/ledger-wallet/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ledger-wallet/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/ledger-wallet"],
      "options": {
        "jestConfig": "packages/ledger-wallet/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["hardware-wallet"]
}
